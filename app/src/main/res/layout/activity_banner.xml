<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent">

    <com.youth.banner.Banner
        android:id="@+id/banner"
        app:indicator_drawable_selected="@drawable/banner_indicator_selected_bg"
        app:indicator_drawable_unselected="@drawable/banner_indicator_normal_bg"
        app:indicator_height="2dp"
        app:indicator_margin="3dp"
        android:layout_width="match_parent"
        android:layout_height="200dp"></com.youth.banner.Banner>
</RelativeLayout>
